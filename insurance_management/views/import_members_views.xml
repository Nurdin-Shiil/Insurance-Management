<odoo>
    <record id="view_insurance_import_members_form" model="ir.ui.view">
        <field name="name">insurance.import.members.form</field>
        <field name="model">insurance.import.members</field>
        <field name="arch" type="xml">
            <form string="Import Members">
                <sheet>
                    <group>
                        <field name="file" />
                    </group>
                    <footer>
                        <button string="Import" type="object" name="action_import"
                            class="btn-primary" />
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>